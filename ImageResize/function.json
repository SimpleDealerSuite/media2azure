{
  "disabled": false,
  "bindings": [
    {
      "name": "myQueueItem",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "imageresizequeue",
      "connection": "clientdatastorage"
    },
    {
      "name": "myInputBlob",
      "type": "blob",
      "path": "images-to-resize/{BlobName}",
      "connection": "clientdatastorage",
      "direction": "in"
    },
    {
      "name": "myOutputBlob",
      "type": "blob",
      "path": "images-sized/{BlobName}",
      "connection": "clientdatastorage",
      "direction": "out"
    }
  ]
}